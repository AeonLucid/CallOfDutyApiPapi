<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>AeonLucid</Authors>
    <Description>An C# client library for the official Call Of Duty API.</Description>
    <RepositoryUrl>https://github.com/AeonLucid/CallOfDutyApiPapi</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>callofduty;blackout;blackops4</PackageTags>
    <PackageProjectUrl>https://github.com/AeonLucid/CallOfDutyApiPapi</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/AeonLucid/CallOfDutyApiPapi/blob/master/LICENSE</PackageLicenseUrl>
    <Copyright>AeonLucid</Copyright>
    <Version>1.0.0</Version>
    <Product>CallOfDutyApiPapi</Product>
    <Company>AeonLucid</Company>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Flurl" Version="2.8.0" />
    <PackageReference Include="Flurl.Http" Version="2.4.0" />
  </ItemGroup>

</Project>
